{
    "compilerOptions": {
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "dist",
      "rootDir": "src",
      "types": ["node"],
      "composite": true,
      "module": "commonjs",

    "moduleResolution": "Node",
      "lib": ["esnext"]
    },

    "exclude": ["node_modules"],
    "include": ["src/**/*.ts"],
  }